{
    "name": "ggomuk/g-bench",
    "description": "PHP Benchmark Component",
    "keywords": ["bechmark", "library", "bench"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ggomuk",
            "email": "mayjsm1206@gmail.com"
        }
